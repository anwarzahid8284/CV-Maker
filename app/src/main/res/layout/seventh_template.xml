            android:textSize="@dimen/_8ssp"
            app:layout_constraintStart_toEndOf="@id/educationToId2"
            app:layout_constraintBottom_toBottomOf="@id/educationToId2"
            app:layout_constraintTop_toTopOf="@id/educationToId2"/>
        <TextView
