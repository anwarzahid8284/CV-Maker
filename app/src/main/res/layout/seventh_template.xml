            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_3sdp"
            android:layout_marginEnd="@dimen/_3sdp"
            android:text="@string/description"
            android:textSize="@dimen/_8ssp"
            app:layout_constraintEnd_toEndOf="@id/lineId2"
            app:layout_constraintStart_toStartOf="@id/profileId"
            app:layout_constraintTop_toBottomOf="@id/experiencedTitleId" />

        <TextView
            android:id="@+id/educationId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:text="@string/education"
            android:textColor="#f2b182"
            android:textSize="@dimen/_8ssp"
            android:fontFamily="@font/timesnewromanbold"
            app:layout_constraintStart_toStartOf="@id/profileId"
            app:layout_constraintTop_toBottomOf="@id/expDescriptionId" />

        <View
            android:id="@+id/lineId4"
            android:layout_width="0dp"
            android:layout_height="@dimen/_1sdp"
            android:layout_marginTop="@dimen/_1sdp"
            android:layout_marginEnd="@dimen/_25sdp"
            android:background="#f3dccd"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/profileId"
            app:layout_constraintTop_toBottomOf="@id/educationId" />
